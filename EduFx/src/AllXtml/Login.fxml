<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <bottom>
      <AnchorPane prefHeight="250.0" prefWidth="250.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="login_name" layoutX="38.0" layoutY="60.0" prefHeight="23.0" prefWidth="175.0" promptText="이름" />
            <PasswordField fx:id="login_pass" layoutX="38.0" layoutY="108.0" prefHeight="23.0" prefWidth="175.0" promptText="비밀번호" />
            <Button fx:id="login_bt" layoutX="87.0" layoutY="165.0" mnemonicParsing="false" prefHeight="51.0" prefWidth="77.0" onAction="#login_bt" text="로그인" />
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
